<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="467.0" prefWidth="779.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mmit.controller.BookViewController">
   <children>
      <Label layoutX="26.0" layoutY="39.0" text="Book List" textFill="#336699">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <ScrollPane layoutX="26.0" layoutY="79.0" prefHeight="297.0" prefWidth="728.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <TableView fx:id="tbl_book" prefHeight="297.0" prefWidth="728.0">
                    <columns>
                      <TableColumn fx:id="col_code" prefWidth="60.0" text="Code" />
                      <TableColumn fx:id="col_title" prefWidth="188.0" text="Title" />
                        <TableColumn fx:id="col_price" prefWidth="85.0" text="Price" />
                        <TableColumn fx:id="col_publishDate" prefWidth="108.0" text="Publish Date" />
                        <TableColumn fx:id="col_category" prefWidth="82.0" text="Category" />
                        <TableColumn fx:id="col_author" prefWidth="95.0" text="Author" />
